                                                            right: '6%',
                                                            top: '4%',
                                                            bottom: '3%',
                                                            containLabel: true
                                                        },
                                                        xAxis: {
                                                            type: 'value',
                                                            axisLine: {
                                                                show: false
                                                            },
                                                            axisTick: {
                                                                show: false
                                                            }
                                                        },
                                                        yAxis: {
                                                            type: 'category',
                                                            data: category,
                                                            splitLine: {show: false},
