                                                <input type="submit" value="{{if eq .Lang "cn"}}确认设置{{else}}Submit{{end}}" class="submitIt simple_buttons" /> &nbsp;&nbsp;&nbsp;&nbsp;
                                                <input type="reset" value="{{if eq .Lang "cn"}}重新选择{{else}}Reset{{end}}" class="submitIt simple_buttons" />  
