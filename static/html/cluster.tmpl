        <script>
                $(document).ready(function(){
                        $("#autoupgradekernel").show();
                        $('#ostype').change(function() {
                                var val = $(this).children('option:selected').val();
                                console.log("ostype:",val);
                                if(val == 'centos7') {
                                        $("#autoupgradekernel").show();
                                }else if(val == 'rhel7') {
                                        $("#autoupgradekernel").show();
                                }else{
                                        $("#autoupgradekernel").hide();
                                }
                        });
                });
