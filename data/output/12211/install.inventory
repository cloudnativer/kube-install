###--------------------------------------General Configuration---------------------------------###

[kissh]
127.0.0.1 ip=127.0.0.1

[master1]
192.168.122.11 ip=192.168.122.11

[k8s:vars]
k8s_install_home="/opt/kube-install/k8s"
k8s_addons_home="/go/src/kube-install/data/output/12211/addons"
software_home="/opt/kube-install"
kipath="/go/src/kube-install"
sub_process_dir="/12211"
rebootime="1"
rebootxt="The operating system will automatically restart to take effect on the cluster configuration."
### Kubernetes Master Configuration ###
ostype="rhel7"
k8sver="1.23"
master_iplist="\"192.168.122.11\""
etcd_initial="192.168.122.11=https://192.168.122.11:2380"
etcd_endpoints="https://192.168.122.11:2379"
ingress_upstream=""
ipvsinit_shell = "ipvsadm -A -t 10.254.0.3:6443 -s rr  && ipvsadm -a -t 10.254.0.3:6443 -r 192.168.122.11:6443 -m"
master1_ip = "192.168.122.11"
master_vip = "192.168.122.11"
master_vport="6443"
k8sdashboardversion="v2.2.0"

### addons_ip配置 ###
addons_ip1="192.168.122.12"
addons_ip2="192.168.122.12"
addons_ip3="192.168.122.12"

### traefik configuration ###
